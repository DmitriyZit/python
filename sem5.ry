# числа фибоначи рекурсия
'''
print('Введите число')
n = int(input())
def fib(n):
    if n in (0, 1):
        return 1
    return fib(n-1) + fib(n-2)


print(fib(n))
'''

n = list(map(int, input().split()))
def xak(n):
    maxx = max(n)
    minn = min(n)
    for i in range(len(n)):
        if n[i] == maxx:
            n[i] = minn
    return n
print(*xak(n))
